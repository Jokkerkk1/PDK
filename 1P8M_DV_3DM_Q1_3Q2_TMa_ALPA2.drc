 err1 = (INT M0G_v_edges M0_NOT_M0C_054 < 0.046 ABUT<90 OPPOSITE REGION) NOT (INT M0G_v_edges M0_NOT_M0C_054_inside_AA >= 0.04 <= 0.041 ABUT<90 OPPOSITE REGION)
 err1 NOT INSIDE SealR_NOT_BULK
}

M0G_O_3 {
