 AOP_M0_c = AOP_M0_v_edges COIN EDGE AOP_M0_ext
 err1 = (AOP_M0_ext WITH EDGE (ENC (AOP_M0_c) M0C_PowerRail_STD == 0.025 ABUT<90 OPPOSITE)) NOT INSIDE GT_P96
 err1 NOT OUTSIDE STDB_all
}

M0C_EX_1b_TBS {
