 Y1_good_step3_i = (Y1_comb_step3 INTERACT 1XV1N >= 3) INTERACT 2XV1N >= 2
 Y1_good_step3_j = ((Y1_comb_step3 INTERACT 1XV1N >= 2) INTERACT 2XV1N >= 1) INTERACT RV1 >= 2
