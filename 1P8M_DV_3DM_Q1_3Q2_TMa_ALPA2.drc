 y1 = HVT_P_convex_eqlt_1_edges COIN EDGE HVT_P_h_edges
 y2 = HVT_P COIN EDGE (EXPAND EDGE (y1 COIN EDGE HVT_P_concave_corner) INSIDE BY 0.001 OUTSIDE BY 0.001)
