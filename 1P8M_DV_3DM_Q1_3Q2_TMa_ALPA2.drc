 Y2_good_step2_f = Y2_comb_step2 INTERACT RV0 >= 5
 Y2_good_step2 = OR Y2_good_step2_a Y2_good_step2_b Y2_good_step2_c Y2_good_step2_d Y2_good_step2_f //; step2: 0.030

