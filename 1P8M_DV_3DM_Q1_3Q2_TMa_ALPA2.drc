 err2_met_up = (dis_up_arm_err INTERACT (1XV3N NOT OUTSIDE dis_up_arm_err)) NOT INTERACT (OR RV3 2XV3N)
 err1_met_down NOT INTERACT Vy3_BAR
}
 
