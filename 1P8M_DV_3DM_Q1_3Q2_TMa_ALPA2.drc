 err4 = GATE CUT HVT_N

 err1 NOT INSIDE GT_P96
 err2 NOT INSIDE GT_P96
 err3 NOT INSIDE GT_P96
 err4 NOT INSIDE GT_P96
}

HVT_N_S_9b {
