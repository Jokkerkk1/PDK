//;based on M0C_R_1_TBS
 chk_M0C = (M0C_h_edges COIN EDGE M0C_PowerRail_STD) NOT OUTSIDE EDGE PowerRail_Area_BusLine
 err1 = INT chk_M0C PowerRail_Area_BusLine < PowerRail_width_half-0.007 ABUT<90 OPPOSITE REGION
