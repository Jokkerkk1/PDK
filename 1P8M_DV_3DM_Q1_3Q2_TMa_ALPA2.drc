 err1 = INT (LVT_P_h_edges_singular_point COIN EDGE LVT_P_concave_corner) LVT_P_h_edges < 0.238 ABUT<90 OPPOSITE REGION
 err1 NOT INSIDE SealR_NOT_BULK

