 y2 = LVT_P_v_edges TOUCH EDGE y1

 waive_corner = INT LVT_P_v_edges LVT_P_90_90_jog_180_192_225_240_135_144_h_edges <= 0.001 ABUT==90 INTERSECTING ONLY REGION
