 Y2_good_step3 = OR Y2_good_step3_a Y2_good_step3_b Y2_good_step3_c Y2_good_step3_d Y2_good_step3_f //; step3: 0.030

 Y2_wide_chk_4 = Y2_wide_chk_3 INTERACT (WIDE_all_raw NOT INTERACT Y2_good_step3)
