 y3 = EXT [ULVT_P_v_edges] >= 0.18 < 0.213 PROJ == 0
 y4 = y2 OR EDGE y3

 z1 = ANGLE ULVT_P_rvs == 0
 z2 = w_all COIN EDGE z1
