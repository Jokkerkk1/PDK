 Y1_good_step3_f = (Y1_comb_step3 INTERACT 1XV1N >= 5) INTERACT RV1 >= 2
 Y1_good_step3_g = ((Y1_comb_step3 INTERACT 1XV1N >= 4) INTERACT 2XV1N >= 1) INTERACT RV1 >= 1
