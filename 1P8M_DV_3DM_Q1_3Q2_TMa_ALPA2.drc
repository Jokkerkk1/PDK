@ Except OCCD, INST, or M0G and AA interact same (M0 NOT M0C)
 waive_1 = (ANGLE (CONVEX EDGE (OR M0G AA M0_NOT_M0C) == 0) == 90) COIN EDGE M0_NOT_M0C
 waive_2 = ((EXPAND EDGE waive_1 OUTSIDE BY 0.001) TOUCH AA) TOUCH M0G
