Core_PW = DRC:1 NOT (DG OR NW)
NW_v_edges = ANGLE NW > 88 <= 90

NW_W_1 {
@ NW width, except INST region >= 0.214um
 err1 = INT NW < NW_MiniWidth ABUT<90 SINGULAR REGION
 err1 NOT INSIDE INST
}

NW_W_2 {
@ IO_NW width >= 0.214um
